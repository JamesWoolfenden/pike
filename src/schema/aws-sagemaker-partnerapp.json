    "list": {
      "permissions": [
        "sagemaker:ListPartnerApps",
        "sagemaker:DescribePartnerApp",
        "sagemaker:ListTags"
      ]
    },
    "read": {
      "permissions": [
        "sagemaker:DescribePartnerApp",
        "sagemaker:ListTags"
      ]
    },
    "update": {
      "permissions": [
        "sagemaker:UpdatePartnerApp",
        "sagemaker:DescribePartnerApp",
        "sagemaker:AddTags",
        "sagemaker:ListTags",
        "sagemaker:DeleteTags"
      ],
      "timeoutInMinutes": 180
    }
  },
  "primaryIdentifier": [
